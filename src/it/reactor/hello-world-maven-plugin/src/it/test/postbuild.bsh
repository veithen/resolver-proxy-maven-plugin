import java.io.*;

BufferedReader in = new BufferedReader(new InputStreamReader(new FileInputStream(new File(basedir, "build.log")), "utf-8"));
String line;
while ((line = in.readLine()) != null) {
    if (line.contains("[WARNING]")) {
        System.out.println("Found warning: " + line);
        return false;
    }
}
in.close();
